<h1> Create a New Post for current_user" </h1>

<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </div>

  <div>
    <%= form.submit "Create Post" %>
  </div>

<% end %>

